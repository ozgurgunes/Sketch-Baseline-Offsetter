{
  "name": "baseline-offsetter",
  "description": "Raise, lower, set or reset the baseline of multiple text layers with a single command.",
  "author": "Ozgur Gunes",
  "authorEmail": "o.gunes@gmail.com",
  "homepage": "https://github.com/ozgurgunes/Sketch-Baseline-Offsetter",
  "license": "MIT",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/ozgurgunes/Sketch-Baseline-Offsetter.git"
  },
  "bugs": {
    "url": "https://github.com/ozgurgunes/Sketch-Baseline-Offsetter/issues"
  },
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Baseline Offsetter",
    "manifest": "src/manifest.json",
    "main": "baseline-offsetter.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/babel-preset": "^0.2.2",
    "@skpm/builder": "^0.7.7",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-sketch": "^0.2.4",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "@ozgurgunes/sketch-plugin-analytics": "^0.4.1",
    "@ozgurgunes/sketch-plugin-ui": "^0.5.0"
  }
}
